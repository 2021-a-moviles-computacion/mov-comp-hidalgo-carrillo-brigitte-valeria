<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BVerEstudiantesColegio">

    <TextView
        android:id="@+id/tv_ver_est_cole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nombre_colegio_est_cole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="@string/txt_nombre"
        app:layout_constraintBottom_toBottomOf="@+id/tv_ver_est_cole"
        app:layout_constraintEnd_toStartOf="@+id/tv_ver_est_cole"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_ver_est_cole"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_ver_estudiante_est_cole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="@string/menItmCol_verEst"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/lv_estudiante_est_cole"
        android:layout_width="226dp"
        android:layout_height="221dp"
        android:layout_marginTop="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tv_ver_est_cole"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_nombre_colegio_est_cole"
        app:layout_constraintTop_toBottomOf="@+id/tv_ver_estudiante_est_cole"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_anadir_est_col"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/menItmCol_registrarEst"
        app:layout_constraintEnd_toEndOf="@+id/lv_estudiante_est_cole"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="@+id/lv_estudiante_est_cole"
        app:layout_constraintTop_toBottomOf="@+id/lv_estudiante_est_cole" />

</androidx.constraintlayout.widget.ConstraintLayout>